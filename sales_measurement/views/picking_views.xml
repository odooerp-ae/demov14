<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="stock_picking_type_kanban" model="ir.ui.view">
            <field name="name">stock.picking.type.kanban</field>
            <field name="model">stock.picking.type</field>
            <field name="inherit_id" ref="stock.stock_picking_type_kanban"/>
            <field name="arch" type="xml">
                <kanban position="inside">
                    <field name="count_delivery_ready" />
                </kanban>
                <xpath expr="//button[@name='get_action_picking_tree_ready']/span[2]" position="replace">
                    <span t-if="record.code.raw_value =='outgoing'">
                        <t t-esc="record.count_delivery_ready.value"/>
                        To Process</span>

                </xpath>
            </field>
        </record>
    </data>
</odoo>